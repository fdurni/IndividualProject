<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.feliciadurni.tt.entity.ProgramsEntity" table="programs" schema="trainingtracker">
        <id name="programId">
            <column name="programId" sql-type="int(11)"/>
        </id>
        <property name="programName">
            <column name="programName" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="programType">
            <column name="programType" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="programDescription">
            <column name="programDescription" sql-type="varchar(150)" length="150" not-null="true"/>
        </property>
        <property name="personId">
            <column name="personId" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>