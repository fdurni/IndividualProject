<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.feliciadurni.tt.entity.PersongoalEntity" table="persongoal" schema="trainingtracker">
        <id name="personGoalId" column="personGoalId"/>
        <property name="goalWeight" column="goalWeight"/>
        <property name="goalDate" column="goalDate"/>
        <property name="personId" column="personId"/>
        <property name="exerciseId" column="exerciseId"/>
    </class>
</hibernate-mapping>