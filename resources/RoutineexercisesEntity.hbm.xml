<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.feliciadurni.tt.entity.RoutineexercisesEntity" table="routineexercises" schema="trainingtracker">
        <id name="routineExercisesId">
            <column name="routineExercisesId" sql-type="int(11)"/>
        </id>
        <property name="expectedSets">
            <column name="expectedSets" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="expectedReps">
            <column name="expectedReps" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="expectedWeight">
            <column name="expectedWeight" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="actualSets">
            <column name="actualSets" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="actualReps">
            <column name="actualReps" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="actualWeight">
            <column name="actualWeight" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="exerciseId">
            <column name="exerciseId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="routineId">
            <column name="routineId" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>