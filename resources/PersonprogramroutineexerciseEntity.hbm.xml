<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.feliciadurni.tt.entity.PersonprogramroutineexerciseEntity" table="personprogramroutineexercise"
           schema="trainingtracker">
        <id name="exerciseId" column="exerciseId"/>
        <property name="expectedReps" column="expectedReps"/>
        <property name="expectedSets" column="expectedSets"/>
        <property name="expectedWeight" column="expectedWeight"/>
        <property name="actualReps" column="actualReps"/>
        <property name="actualSets" column="actualSets"/>
        <property name="actualWeight" column="actualWeight"/>
        <property name="personProgramRoutineId" column="personProgramRoutineId"/>
        <property name="programRoutineExerciseId" column="programRoutineExerciseId"/>
    </class>
</hibernate-mapping>