<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.feliciadurni.tt.entity.ExercisesEntity" table="exercises" schema="trainingtracker">
        <id name="exerciseId" column="exerciseId"/>
        <property name="exerciseName" column="exerciseName"/>
        <property name="exerciseType" column="exerciseType"/>
        <property name="exerciseDescription" column="exerciseDescription"/>
    </class>
</hibernate-mapping>